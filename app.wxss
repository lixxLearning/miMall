/**app.wxss**/
@import "./lib/weui.wxss";

/* 字体抗锯齿渲染 */
page{
  background: #ebebeb;
  -webkit-font-smoothing: antialiased;
  box-sizing: border-box;
}

.container {
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%; 
  box-sizing: border-box;
} 

/*隐藏滚动条*/
::-webkit-scrollbar{    
  height: 0;
  width: 0;
  color: transparent;
}
